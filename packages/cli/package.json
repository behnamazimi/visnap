{
  "name": "@visnap/cli",
  "version": "0.9.0",
  "description": "Visual Testing Tool - CLI for visual regression testing",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "visnap": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.9",
    "@visnap/core": "*",
    "@visnap/reporter": "*",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "figlet": "^1.7.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^12.9.6",
    "inquirer-checkbox-plus-plus": "^1.1.0",
    "open": "^10.0.2",
    "ora": "^8.0.1",
    "table": "^6.8.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@visnap/eslint-config": "*",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.8",
    "tsup": "^8.3.5",
    "typescript": "5.9.3",
    "vitest": "^4.0.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/behnamazimi/visnap.git",
    "directory": "packages/cli"
  },
  "homepage": "https://visnap.dev",
  "bugs": {
    "url": "https://github.com/behnamazimi/visnap/issues"
  },
  "keywords": [
    "visual-testing",
    "screenshot",
    "regression-testing",
    "storybook",
    "playwright",
    "cli",
    "testing-tool"
  ],
  "license": "MIT",
  "author": "visnap contributors"
}
