{
  "name": "@visnap/cli",
  "version": "0.3.0",
  "description": "Visual Testing Tool - CLI for visual regression testing",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "visnap": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.9",
    "@visnap/core": "^0.3.0",
    "@visnap/reporter": "^0.3.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.0",
    "figlet": "^1.7.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^12.9.6",
    "inquirer-checkbox-autocomplete-prompt": "^0.2.0",
    "open": "^10.0.2",
    "ora": "^8.0.1",
    "table": "^6.8.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@visnap/eslint-config": "*",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "msw": "^2.6.6",
    "tsup": "^8.3.5",
    "typescript": "5.9.2",
    "vitest": "^2.1.8"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
