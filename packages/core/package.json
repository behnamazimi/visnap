{
  "name": "@visnap/core",
  "version": "0.9.0",
  "description": "Visual Testing Tool - Core library for visual regression testing",
  "type": "module",
  "main": "./dist/core.cjs",
  "module": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@visnap/fs-adapter": "*",
    "@visnap/protocol": "*",
    "arktype": "^2.1.25",
    "bundle-require": "^5.1.0",
    "chalk": "^5.6.2",
    "esbuild": "^0.25.0",
    "lodash": "^4.17.21",
    "odiff-bin": "^4.0.0",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.0",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.8",
    "@visnap/eslint-config": "*",
    "tsup": "^8.3.5",
    "typescript": "5.9.3",
    "vitest": "^4.0.16"
  },
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/behnamazimi/visnap.git",
    "directory": "packages/core"
  },
  "homepage": "https://visnap.dev",
  "bugs": {
    "url": "https://github.com/behnamazimi/visnap/issues"
  },
  "keywords": [
    "visual-testing",
    "screenshot",
    "regression-testing",
    "core",
    "testing-engine",
    "orchestration"
  ],
  "license": "MIT",
  "author": "visnap contributors"
}
