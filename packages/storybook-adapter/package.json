{
  "name": "@visual-testing-tool/storybook-adapter",
  "version": "0.1.0",
  "description": "Storybook test-case adapter for visual-testing-tool",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@visual-testing-tool/protocol": "*",
    "serve-handler": "^6.1.5"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/serve-handler": "^6.1.4",
    "@visual-testing-tool/eslint-config": "*",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "tsup": "^8.3.5",
    "typescript": "5.9.2",
    "vitest": "^2.1.8"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}


