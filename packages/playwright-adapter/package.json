{
  "name": "@visnap/playwright-adapter",
  "version": "0.6.0",
  "description": "Playwright browser adapter for visnap",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "playwright": ">=1.40.0",
    "playwright-core": ">=1.40.0"
  },
  "dependencies": {
    "@visnap/protocol": "*",
    "arktype": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@visnap/eslint-config": "*",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "tsup": "^8.3.5",
    "typescript": "5.9.3",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/behnamazimi/visnap.git",
    "directory": "packages/playwright-adapter"
  },
  "homepage": "https://visnap.dev",
  "bugs": {
    "url": "https://github.com/behnamazimi/visnap/issues"
  },
  "keywords": [
    "visual-testing",
    "playwright",
    "browser-automation",
    "adapter",
    "testing"
  ],
  "license": "MIT",
  "author": "visnap contributors"
}
