{
  "extends": "../core/tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2020",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "types": ["node"],
    "noEmit": false
  },
  "include": ["src/**/*"],
  "exclude": ["**/node_modules/**", "dist"]
}


