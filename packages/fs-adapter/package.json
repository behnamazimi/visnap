{
  "name": "@visnap/fs-adapter",
  "version": "0.3.0",
  "description": "Filesystem storage adapter for visnap visual testing",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "visual-testing",
    "screenshot",
    "filesystem",
    "storage",
    "adapter"
  ],
  "author": "visnap",
  "license": "MIT",
  "dependencies": {
    "@visnap/protocol": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@visnap/eslint-config": "^0.3.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
